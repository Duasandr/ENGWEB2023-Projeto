extends ../index 

block middle-column 
    // Middle Column
    .w3-col.m7
        .w3-row-padding

        // Posts
        if(data.length == 0)
            .w3-container.w3-red
                p Nenhum lugar para listar

        .w3-container.w3-card.w3-white.w3-round.w3-margin
            h1 Lugares
            each lugar in data
                a(href="/ruas/lugares/" + lugar._id)=lugar._id
                br 
                